{
  "mcpServers": {
    "cloudflare": {
      "command": "node",
      "args": [
        "path/to/cloudflare/server",
        "run",
        "your-cloudflare-id"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": ".cursor/memory.json"
      }
    }
  }
} 